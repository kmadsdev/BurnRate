    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "jsdom": "^23.2.0",
    "vite": "^5.0.8",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^1.2.0"
  }
